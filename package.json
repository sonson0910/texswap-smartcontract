{
  "name": "@pulsefinance/dex-contract",
  "version": "0.1.13",
  "description": "Compact contracts for Pulse's Midnight DEX",
  "exports": {
    "./amm": "./dist/amm/contract/index.js",
    "./faucet": "./dist/faucet/contract/index.js",
    "./faucet-amm": "./dist/faucet-amm/contract/index.js",
    "./faucet-amm-v2": "./dist/faucet-amm-v2/contract/index.js"
  },
  "scripts": {
    "build": "pnpm run build:faucet-amm-unshielded",
    "build:amm": "compactc ./src/AMM.compact ./dist/amm",
    "build:faucet-amm": "compactc ./src/FaucetAMM.compact ./dist/faucet-amm",
    "build:faucet-amm-v2": "compact compile ./src/FaucetAMMv2.compact ./dist/faucet-amm-v2",
    "build:faucet-amm-unshielded": "compact compile ./src/FaucetAMMUnshielded.compact ./dist/faucet-amm-unshielded",
    "build:faucet": "compactc ./src/Faucet.compact ./dist/faucet",
    "build:deploy": "compact compile ./src/FaucetAMMUnshielded.compact build/FaucetAMMUnshielded && mkdir -p deploy-cli/dist/faucet-amm-unshielded && cp -r build/FaucetAMMUnshielded/* deploy-cli/dist/faucet-amm-unshielded/",
    "test": "bun test"
  },
  "keywords": [],
  "author": "Christian Schmitz",
  "license": "MIT",
  "packageManager": "pnpm@10.20.0",
  "devDependencies": {
    "@types/bun": "^1.3.5"
  },
  "dependencies": {
    "@midnight-ntwrk/compact-runtime": "0.14.0",
    "@midnight-ntwrk/onchain-runtime-v2": "2.0.0"
  }
}